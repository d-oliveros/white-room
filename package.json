  "scripts": {
    "start": "nx run-many --target=serve --parallel=8",
    "build": "nx run-many --target=build --parallel=8 --verbose",
    "test": "nx run-many --target=test --parallel=8 --verbose",
    "test:scrapers": "nx run-many --target=scrape --parallel=8 --verbose",
    "lint": "nx run-many --target=lint --parallel=8 --verbose",
    "pre-commit": "nx affected --target=lint,test --base=origin/main --parallel=8 --verbose",
    "playwright:show-trace": "npx playwright show-trace ./trace.zip",
    "playwright:codegen": "npx playwright install chromium && npx playwright codegen",
    "migrations:dev:run": "dotenv -f .env.dev run nx run migrations:run",
    "migrations:dev:seed": "dotenv -f .env.dev run nx run migrations:seed:admin",
    "migrations:prod:run": "dotenv -f .env.prod run nx run migrations:run",
    "migrations:prod:seed": "dotenv -f .env.prod run nx run migrations:seed:admin",
