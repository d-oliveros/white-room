{
  "name": "white-room",
  "version": "2.0.0",
  "type": "module",
  "private": true,
  "engines": {
    "node": "22.3.0",
    "npm": "10.8.1"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "run-p dev-node dev-webpack",
    "dev-node": "clear && node --watch-path=./src --env-file=.env --env-file=.env.default --import 'data:text/javascript,import { register } from \"node:module\"; import { pathToFileURL } from \"node:url\"; register(\"ts-node/esm\", pathToFileURL(\"./\")); register(\"./babel-esm-loader.js\", pathToFileURL(\"./\")); ' app/init.ts",
    "dev-webpack": "clear && dotenv -e .env -e .env.default webpack serve",
    "dev-debug": "cross-env DEBUG=white-room* npm run dev",
    "build": "run-p build-babel build-webpack build-storybook",
    "build-e2e": "run-p build-babel build-webpack",
    "build-babel": "babel app --out-dir lib --keep-file-extension ",
    "build-webpack": "webpack --config webpack.config.js",
    "build-storybook": "cross-env NODE_ENV=development build-storybook -s ./public -c ./.storybook -o ./.storybook/dist",
    "webpack-analyze-bundle": "npm run build-webpack && webpack --config webpack.config.js --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json build",
    "serve": "node init",
    "lint": "eslint --max-warnings 0 --ignore-path .gitignore . && clear && echo '[lint]: Done.'",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && echo '[lint-fix]: Done.'",
    "test-build": "npm run build && USE_BUILD=true node init",
    "test": "POSTGRES_DATABASE=whiteroom_dev mocha --ui bdd --recursive --exit",
    "test-watch": "npm run test -- --watch",
    "script": "node src/scripts/load",
    "cron-task": "cross-env DEBUG=whiteroom* dotenv -e .env -e .env.default node util/load run-cron",
    "db-create-migration": "typeorm-ts-node-esm migration:generate -d app/data-source.js migrations/$npm_config_name && npm run db-migrate-latest",
    "db-migrate-latest": "typeorm-ts-node-esm migration:run -d app/data-source.js",
    "db-drop": "docker compose down && docker compose up -d && npm run wait-for-db",
    "db-reset": "npm run db-drop && npm run db-migrate-latest && npm run knex-load-seeds",
    "db-clear": "echo \"TBD: Implement using clear-db.js\"",
    "wait-for-db": "echo \"Waiting for PostgreSQL to be ready...\" && until docker-compose exec -T postgres pg_isready -h localhost -p 5432 > /dev/null 2>&1; do sleep 0.1; done && echo \"PostgreSQL is ready.\";",
    "knex-load-seeds": "dotenv -e .env -e .env.default knex seed:run",
    "cypress:install": "npm install --no-save cypress @cypress/webpack-preprocessor",
    "cypress:open": "cypress open --e2e --browser electron",
    "cypress:test": "cypress run",
    "prepush": "npm run lint",
    "storybook": "start-storybook -s ./public -p 6006",
    "type-check": "tsc --noEmit",
    "precommit": "npm run type-check"
  },
  "dependencies": {
    "@aws-sdk/client-lambda": "^3.621.0",
    "@aws-sdk/client-s3": "^3.621.0",
    "@dr.pogodin/babel-plugin-react-css-modules": "^6.13.2",
    "@segment/analytics-node": "^2.1.2",
    "@tanstack/react-query": "^5.51.16",
    "baobab": "^2.6.1",
    "bcrypt": "^5.1.1",
    "body-parser": "^1.20.2",
    "bullmq": "^5.12.0",
    "classnames": "^2.5.1",
    "cookie-parser": "^1.4.6",
    "copy-to-clipboard": "^3.3.3",
    "dayjs": "^1.11.12",
    "debug": "^4.3.6",
    "express": "^4.19.2",
    "flatted": "^3.3.1",
    "formidable": "^3.5.1",
    "formik": "^2.4.6",
    "he": "^1.2.0",
    "helmet": "^7.1.0",
    "http-proxy-middleware": "^3.0.0",
    "ioredis": "^5.4.1",
    "ip-regex": "^5.0.0",
    "isbot": "^5.1.13",
    "jsesc": "^3.0.2",
    "jsonwebtoken": "^9.0.2",
    "knex": "^3.1.0",
    "knex-postgis": "^0.14.3",
    "lodash": "^4.17.21",
    "mime": "^4.0.4",
    "mobile-detect": "^1.4.5",
    "ms": "^2.1.3",
    "node-cron": "^3.0.3",
    "objectdiff": "^1.1.0",
    "pg": "^8.12.0",
    "pino": "^9.3.2",
    "pluralize": "^8.0.0",
    "prop-types": "^15.8.1",
    "react": "^18.3.1",
    "react-autosuggest": "^10.1.0",
    "react-datetime": "^3.2.0",
    "react-dom": "^18.3.1",
    "react-leaflet": "^4.2.1",
    "react-markdown": "^9.0.1",
    "react-number-format": "^5.4.0",
    "react-portal": "^4.2.2",
    "react-responsive": "^10.0.0",
    "react-router": "^6.25.1",
    "react-router-dom": "^6.25.1",
    "serialize-error": "^11.0.3",
    "sitemap": "^8.0.0",
    "type-check": "^0.4.0",
    "ua-parser-js": "^1.0.38",
    "uuid": "^10.0.0",
    "zod": "^3.23.8"
