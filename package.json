{
  "name": "white-room",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "16.10.0",
    "npm": "7.24.0"
  },
  "scripts": {
    "start": "npm run dev",
    "dev": "npm-run-all --parallel dev-node dev-webpack",
    "dev-node": "DEBUG=whiteroom* nodemon -w ./src/api -w ./src/client -w ./src/common -w ./src/cron -w ./src/queue -w ./src/server -i node_modules/ -i test/ -i cypress/ -e js,hbs -x node init",
    "dev-webpack": "webpack serve --mode=development --hot --config webpack.dev-server.config.js",
    "build": "npm-run-all --parallel build-babel build-webpack build-storybook",
    "build-e2e": "npm-run-all --parallel build-babel build-webpack",
    "build-babel": "babel src --out-dir lib",
    "build-webpack": "webpack --config webpack.config.js",
    "build-storybook": "NODE_ENV=development build-storybook -s ./public -c ./.storybook -o ./.storybook/dist",
    "webpack-analyze-bundle": "npm run build-webpack && webpack --config webpack.config.js --profile --json > build/stats.json && webpack-bundle-analyzer build/stats.json build",
    "serve": "node init",
    "lint": "eslint --ignore-path .gitignore . && echo \"[lint]: Done.\"",
    "lint-strict": "eslint --max-warnings 0 --ignore-path .gitignore . && echo \"[lint-strict]: Done.\"",
    "lint-fix": "eslint --fix --ignore-path .gitignore . && echo \"[lint-fix]: Done.\"",
    "test-build": "npm run build && USE_BUILD=true node init",
    "dotenv": "dotenv",
    "test": "NODE_ENV=test mocha --require _initEnv.js --ui bdd --recursive --exit",
    "test-watch": "npm run test -- --watch",
    "script": "NODE_ENV=cli node util/load",
    "reinstall-modules": "rm -rf node_modules && npm i",
    "cron-task": "DEBUG=whiteroom* dotenv -e .env -e .env.default node util/load run-cron",
    "knex-create-migration": "knex migrate:make $1 -x js",
    "knex-create-seed": "knex seed:make",
    "knex-load-seeds": "dotenv -e .env -e .env.default knex seed:run",
    "knex-migrate-latest": "dotenv -e .env -e .env.default knex migrate:latest",
    "knex-migrate-down": "dotenv -e .env -e .env.default knex migrate:down",
    "knex-migrate-rollback": "dotenv -e .env -e .env.default knex migrate:rollback",
    "knex-migrate-rollback-one": "dotenv -e .env -e .env.default knex migrate:down",
    "knex-migrate-current": "npm run knex-migrate-rollback && npm run knex-migrate-latest && npm run knex-load-seeds",
    "knex-migrate-unlock": "dotenv -e .env -e .env.default knex migrate:unlock",
    "knex-migrate-list": "dotenv -e .env -e .env.default knex migrate:list",
    "reset-db": "npm run script clear-db && npm run knex-migrate-latest && npm run knex-load-seeds",
    "cypress:install": "npm install --no-save cypress@12.17.2 @cypress/webpack-preprocessor",
    "cypress:open": "cypress open --e2e --browser electron",
    "cypress:test": "cypress run",
    "prepush": "npm run lint-strict",
    "storybook": "start-storybook -s ./public -p 6006"
  },
